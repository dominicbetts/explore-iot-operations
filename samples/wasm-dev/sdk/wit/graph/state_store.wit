interface state-store {
	variant state-store-error {
		request-error(string),
		timeout,
		protocol,
		internal,
	}

	get: func(key: list<u8>) -> result<option<list<u8>>, state-store-error>;

	set: func(key: list<u8>, value: list<u8>) -> result<_, state-store-error>;
}
world state-store-use {
	import state-store;
}
